{
	"$schema": "https://schemas.wp.org/wp/6.0/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "page-white",
			"title": "Background white",
			"postTypes": [
				"page"
			]
		}
	],
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "760px",
			"wideSize": "1100px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "roboto"
				},
				{
					"fontFamily": "\"Roboto\", serif",
					"name": "Roboto",
					"slug": "roboto",
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "light",
							"src": [ "file:./assets/fonts/roboto/Roboto-Light.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/roboto/Roboto-Regular.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "medium",
							"src": [ "file:./assets/fonts/roboto/Roboto-Medium.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "bold",
							"src": [ "file:./assets/fonts/roboto/Roboto-Bold.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "900",
							"fontStyle": "normal",
							"fontStretch": "black",
							"src": [ "file:./assets/fonts/roboto/Roboto-Black.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontStretch": "light",
							"src": [ "file:./assets/fonts/roboto/Roboto-LightItalic.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/roboto/Roboto-Italic.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontStretch": "medium",
							"src": [ "file:./assets/fonts/roboto/Roboto-MediumItalic.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontStretch": "bold",
							"src": [ "file:./assets/fonts/roboto/Roboto-BoldItalic.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "900",
							"fontStyle": "italic",
							"fontStretch": "black",
							"src": [ "file:./assets/fonts/roboto/Roboto-BlackItalic.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": "1.1rem",
					"slug": "small"
				},
				{
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"size": "2.25rem",
					"slug": "x-large"
				}
			]
		},
		"color": {
			"palette": [
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#736CED",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#EABE3F",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#4D4855",
					"name": "Tertiary"
				},
				{
					"slug": "textsecondary",
					"color": "#e5e5e5",
					"name": "Text Seconday"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "background-light",
					"color": "#2e2e2e",
					"name": "Dark Secondary"
				}
			]
		},
		"custom": {
			"typography": {
				"fontFamily": {
					"Roboto":"var(--wp--preset--font-family--roboto)"
				},
				"font-size": {
					"huge": "clamp(1.5rem, 3vw, 1.75rem)",
					"gigantic": "clamp(1.75rem, 4vw, 2.25rem)",
					"colossal": "clamp(2.25rem, 6vw, 3.25rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.6
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "5px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "bold"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--foreground)",
			"text": "var(--wp--preset--color--white)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "600",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--small)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}
