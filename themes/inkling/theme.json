{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"position": {
			"sticky": true
		},
		"layout": {
			"contentSize": "705px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "0.5rem",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "1rem",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "1.7rem",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "2.5rem",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "3.44rem",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "4.63rem",
					"slug": "70",
					"name": "7"
				},
				{
					"size": "6rem",
					"slug": "80",
					"name": "8"
				}
			]
		},
		"color": {
			"custom": true,
			"defaultPalette": false,
			"palette": [
				{
					"color": "rgb(255,255,255)",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "rgb(55, 53, 47)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgb(35, 131, 226)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#787774",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#9F6B53",
					"name": "Brown",
					"slug": "brown"
				},
				{
					"color": "#D9730D",
					"name": "Orange",
					"slug": "orange"
				},
				{
					"color": "#CB912F",
					"name": "Yellow",
					"slug": "yellow"
				},
				{
					"color": "#448361",
					"name": "Green",
					"slug": "green"
				},
				{
					"color": "#337EA9",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#9065B0",
					"name": "Purple",
					"slug": "purple"
				},
				{
					"color": "#C14C8A",
					"name": "Pink",
					"slug": "pink"
				},
				{
					"color": "#D44C47",
					"name": "Red",
					"slug": "red"
				},
				{
					"color": "rgba(255,255,255,0.9)",
					"name": "Tint",
					"slug": "tint"
				},
				{
					"color": "rgba(0,0,0,0.10)",
					"name": "Tint (Alt)",
					"slug": "tint-alt"
				}
			]
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Normal",
					"slug": "normal"
				},
				{
					"fontFamily": "Serif",
					"slug": "serif",
					"fontFace": [
						{
							"fontFamily": "Serif",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lora_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Serif",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/lora_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Serif",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/lora_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Serif",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lora_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Serif",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lora_italic_400.ttf"
							]
						},
						{
							"fontFamily": "Serif",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/lora_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/lora_italic_600.ttf"
							]
						},
						{
							"fontFamily": "Serif",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lora_italic_700.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Mono",
					"slug": "mono",
					"fontFace": [
						{
							"fontFamily": "Mono",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/iAWriterMonoS-Regular.ttf"
							]
						},
						{
							"fontFamily": "Mono",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/iAWriterMonoS-Bold.ttf"
							]
						},
						{
							"fontFamily": "Mono",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/iAWriterMonoS-Italic.ttf"
							]
						},
						{
							"fontFamily": "Mono",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/iAWriterMonoS-BoldItalic.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid":false,
					"size": "0.50rem",
					"name": "1",
					"slug": "1"
				},
				{
					"fluid":false,
					"size": "0.75rem",
					"name": "2",
					"slug": "2"
				},
				{
					"fluid":false,
					"size": "0.875rem",
					"name": "3",
					"slug": "3"
				},
				{
					"fluid":false,
					"size": "1rem",
					"name": "4",
					"slug": "4"
				},
				{
					"fluid":true,
					"size": "1.25rem",
					"name": "5",
					"slug": "5"
				},
				{
					"fluid":true,
					"size": "1.5rem",
					"name": "6",
					"slug": "6"
				},
				{
					"fluid":true,
					"size": "1.875rem",
					"name": "7",
					"slug": "7"
				},
				{
					"fluid":true,
					"size": "2.5rem",
					"name": "8",
					"slug": "8"
				},
				{
					"fluid":true,
					"size": "3rem",
					"name": "9",
					"slug": "9"
				}
			]
		},
		"custom": {
			"styleOpacity": "0.65"
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--normal)",
			"lineHeight": "1.5",
			"fontSize": "var(--wp--preset--font-size--4)"
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"left": "clamp(var(--wp--preset--spacing--30), 4vw, var(--wp--preset--spacing--80))",
				"right": "clamp(var(--wp--preset--spacing--30), 4vw, var(--wp--preset--spacing--80))",
				"top": "0",
				"bottom": "var(--wp--preset--spacing--80)"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				},
				"border": {
					"bottom": {
						"color":"var(--wp--preset--color--gray)",
						"style": "solid",
						"width": "0.05rem"
					}
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2)",
					"lineHeight": "1.4"
				}
			},
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2)",
					"lineHeight": "1.1",
					"fontWeight": "500"
				},
				"color": {
					"text": "var(--wp--preset--color--white)",
					"background": "var(--wp--preset--color--primary)"
				},
				"border": {
					"radius": "0.1875rem",
					"width": "1px",
					"color": "rgba(55, 53, 47, 0.16)",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"left": "0.5rem",
						"right": "0.5rem",
						"top": "0.5rem",
						"bottom": "0.5rem"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.2"
				}	
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--7)"
				}	
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--6)"
				}	
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--5)"
				}	
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--4)"
				}	
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2)"
				}	
			}
		},
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						"border": {
							"style": "none"
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"border": {
							"style": "none"
						}
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"radius": "0.1875rem",
							"width": "0"
						},
						"spacing": {
							"padding": {
								"left": "calc(0.5rem + 1px)",
								"right": "calc(0.5rem + 1px)",
								"top": "calc(0.5rem + 1px)",
								"bottom": "calc(0.5rem + 1px)"
							}
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"style": "none"
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"style": "solid",
						"color": "var(--wp--preset--color--contrast)",
						"width": "0.1875rem"
					}
				},
				"spacing": {
					"padding": {
						"left": "0.875rem",
						"right": "0.875rem"
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3)"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"border": {
							"bottom": {
								"style": "none",
								"width": "0"
							},
							"radius": "0.1875rem"
						},
						"spacing": {
							"padding": {
								"top": "0.25rem",
								"bottom": "0.25rem",
								"left": "0.5rem",
								"right": "0.5rem"
							}
						},
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--tint-alt)"
							}
						}
					}
				}
			},
			"core/image": {
				"elements": {
					"link": {
						"border": {
							"bottom": {
								"style": "none"
							}
						}
					}
				}
			},
			"core/post-featured-image": {
				"elements": {
					"link": {
						"border": {
							"bottom": {
								"style": "none"
							}
						}
					}
				}
			},
			"core/site-logo": {
				"elements": {
					"link": {
						"border": {
							"bottom": {
								"style": "none"
							}
						}
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "general-query",
			"title": "General Query"
		},
		{
			"area": "uncategorized",
			"name": "post-layout",
			"title": "Default Post Layout"
		}
	],
	"customTemplates": [
		{
			"name": "serif",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Serif"
		},
		{
			"name": "mono",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Mono"
		}
	],
	"version": 2
}