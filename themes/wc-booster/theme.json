					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"lineHeight": "1.625rem",
		            "fontWeight": "500"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--hover) !important"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--hover) !important"
							}
						},
						":active": {
							"color": {
