{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "no-title-space",
			"postTypes": [ "page" ],
			"title": "No Title and Space"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customGradient": true,
			"palette": [
				{
					"color": "#212529",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0047ab",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#212B36",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#F1F4F4",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#F7F7F9",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#cccccc",
					"name": "Quinary",
					"slug": "quinary"
				},
				{
					"color": "#2f2f2f",
					"name": "Senary",
					"slug": "senary"
				},
				{
					"color": "transparent",
					"name": "Transparent",
					"slug": "transparent"
				},
				{
					"color": "currentColor",
					"name": "Current Color",
					"slug": "current-color"
				},
				{
					"color": "inherit",
					"name": "Inherit",
					"slug": "inherit"
				}
			],
			"gradients": [
				{
					"slug": "primary",
					"gradient": "linear-gradient(180deg, #f1f4f4 50%, #ffffff 50%);",
					"name": "Primary"
				}
			]
		},
		"layout": {
			"contentSize": "760px",
			"wideSize": "1140px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"customSpacingSize": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "2X Small",
					"size": "8px",
					"slug": "xx-small"
				},
				{
					"name": "X Small",
					"size": "clamp(0.75rem, 0.659rem + 0.45vw, 1rem)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(1.125rem, 0.989rem + 0.68vw, 1.5rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 1.318rem + 0.91vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2.125rem, 1.807rem + 1.59vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "X Large",
					"size": "clamp(2.625rem, 2.125rem + 2.5vw, 4rem)",
					"slug": "x-large"
				},
				{
					"name": "2X Large",
					"size": "clamp(3.688rem, 2.847rem + 4.2vw, 6rem)",
					"slug": "xx-large"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "inter",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/Inter-Variable.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "WorkSans, sans-serif",
					"name": "WorkSans",
					"slug": "worksans",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "WorkSans",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/WorkSans-Variable.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "X Small",
					"size": "0.63rem",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "0.84rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.13rem"
					},
					"name": "Base",
					"size": "1.13rem",
					"slug": "base"
				},
				{
					"fluid": {
						"min": "1.2rem",
						"max": "1.5rem"
					},
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.44rem",
						"max": "2rem"
					},
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.73rem",
						"max": "2.66rem"
					},
					"name": "X Large",
					"size": "2.66rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.07rem",
						"max": "3.55rem"
					},
					"name": "2X Large",
					"size": "3.55rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "2.49rem",
						"max": "4.73rem"
					},
					"name": "3X Large",
					"size": "4.73rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"min": "2.99rem",
						"max": "6.31rem"
					},
					"name": "4X Large",
					"size": "6.31rem",
					"slug": "xxxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "5px"
				},
				"typography": {
					"lineHeight": "1"
				},
				"variations": {
					"outline": {
						"color": {
							"text": "var:preset|color|primary"
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|x-large"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontWeight": "500"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-date": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/post-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/post-navigation-link": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/comment-date": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/comment-reply-link": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-edit-link": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/list": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0"
					}
				}
			}
		},
		"color": {
			"background": "var:preset|color|white",
			"text": "var:preset|color|base"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|primary"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"lineHeight": "1.4"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "1.5"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "1.6"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "1.6"
				}
			},
			"heading": {
				"color": {
					"text": "var:preset|color|secondary"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|worksans",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|primary"
				}
			}
		},
		"spacing": {
			"blockGap": "24px",
			"padding": {
				"bottom": "0px",
				"left": "24px",
				"right": "24px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|worksans",
			"fontSize": "var:preset|font-size|base",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "content",
			"title": "Content"
		},
		{
			"area": "uncategorized",
			"name": "content-single",
			"title": "Content Single"
		}
	]
}
