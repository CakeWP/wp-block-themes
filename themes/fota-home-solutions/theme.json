{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": true,
      "customGradient": true,
      "customDuotone": true,
      "link": true,
      "palette": [
        {
          "slug": "primary",
          "color": "#142257",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#15B2F5",
          "name": "Secondary"
        },
        {
          "slug": "tertiary",
          "color": "#1F3584",
          "name": "Tertiary"
        },
        {
          "slug": "tertiary-light",
          "color": "#1587C5",
          "name": "Tertiary Light"
        },
        {
          "slug": "accent",
          "color": "#202D5F",
          "name": "Accent"
        },
        {
          "slug": "accent-lite",
          "color": "#9EA3B5",
          "name": "Accent Lite"
        },
        {
          "slug": "background-alt",
          "color": "#F3F5F9",
          "name": "Background Alternative"
        },
        {
          "slug": "foreground",
          "color": "#545971",
          "name": "Foreground"
        },
        {
          "slug": "background",
          "color": "#ffffff",
          "name": "Background"
        },
        {
          "name": "Heading Color",
          "color": "#171921",
          "slug": "heading-color"
        },
        {
          "name": "Sub Heading Color",
          "color": "#9EA3B5",
          "slug": "sub-heading-color"
        },
        {
          "name": "Foreground Alternative",
          "color": "#FEFEFE",
          "slug": "foreground-alt"
        },
        {
          "name": "Border Color",
          "color": "#1F2D62",
          "slug": "border-color"
        },
        {
          "color": "#ffffff00",
          "name": "Transparent",
          "slug": "transparent"
        }
      ],
      "duotone": [
        {
          "colors": ["#1F3584", "#ffffff"],
          "slug": "primary-white",
          "name": "Primary White"
        },
        {
          "colors": ["#ffffff", "#1F3584"],
          "slug": "white-primary",
          "name": "White Primary"
        },
        {
          "colors": ["#15B2F5", "#ffffff"],
          "slug": "secondary-white",
          "name": "Secondary White"
        },
        {
          "colors": ["#ffffff", "#15B2F5"],
          "slug": "white-secondary",
          "name": "White Secondary"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
      "fontSize": "var(--wp-preset--font-size--small)"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
          "fontStyle": "normal",
          "fontWeight": "var(--wp--preset--font-weight--regular)",
          "lineHeight": "var(--wp--preset--line-height--xxSmall)"
        },
        "color": {
          "text": "var(--wp--preset--color--heading-color)",
          "background": "var(--wp--preset--color--transparent)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    }
  }
}
