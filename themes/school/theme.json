{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#1d1f35",
						"#6cace4"
					],
					"name": "Dark blue and light blue",
					"slug": "dark-blue-and-light-blue"
				},
				{
					"colors": [
						"#1d1f35",
						"#c7c0af"
					],
					"name": "Dark blue and beige",
					"slug": "dark-blue-and-beige"
				}
			],
			"gradients": [],
			"link": true,
			"palette": [
				{
					"color": "#525fe1",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f79f8b",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#44c553",
					"name": "Success",
					"slug": "success"
				},
				{
					"color": "#17a2b8",
					"name": "Info",
					"slug": "info"
				},
				{
					"color": "#ffc107",
					"name": "Warning",
					"slug": "warning"
				},
				{
					"color": "#dc3545",
					"name": "Danger",
					"slug": "danger"
				},
				{
					"color": "#ad6edd",
					"name": "Purple",
					"slug": "purple"
				},
				{
					"color": "#ff977a",
					"name": "Salmon",
					"slug": "salmon"
				},
				{
					"color": "#35bdff",
					"name": "Cyan",
					"slug": "cyan"
				},
				{
					"color": "#ced4da",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#495057",
					"name": "Gray Dark",
					"slug": "gray-dark"
				},
				{
					"color": "#502c6c",
					"name": "Indigo",
					"slug": "indigo"
				},
				{
					"color": "#fbb500",
					"name": "Orange",
					"slug": "orange"
				},
				{
					"color": "#f8f9fa",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#343a40",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#1f1b2d",
					"name": "Dark Black",
					"slug": "dark-black"
				},
				{
					"color": "#000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": null
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/montserrat/Montserrat-VariableFont_wght.woff2" ]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/montserrat/SourceSerif4Variable-Italic.ttf.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "1.25rem",
					"slug": "normal"
				},
				{
					"name": "medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"name": "Extra large",
					"size": "3rem",
					"slug": "extra-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "4px",
					"width": "0px"
				},
				"color": {
					"background": "var:preset|color|primary"
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/latest-posts": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"border": {
							"radius": "4px"
						},
						"color": {
							"background": "var(--wp--preset--color--light-grey)"
						},
						"spacing": {
							"padding": {
								"bottom": "calc(.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(.667em + 2px)"
							}
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "2.5rem"
					}
				}
			},
			"core/post-navigation-link": {
				"elements": {
					"link": {
						"border": {
							"radius": "40px"
						},
						"color": {
							"background": "var(--wp--preset--color--light-grey)"
						},
						"spacing": {
							"padding": {
								"bottom": "calc(.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(.667em + 2px)"
							}
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"border": {
							"radius": "4px"
						},
						"color": {
							"background": "var(--wp--preset--color--light-grey)"
						},
						"spacing": {
							"padding": {
								"bottom": "calc(.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(.667em + 2px)"
							}
						}
					}
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"bottom": "0px"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var:preset|color|dark-black"
		},
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--dark-black)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--dark-black)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--dark-black)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--dark-black)"
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--preset--color--dark-black)"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--dark-black)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-blue)"
				}
			}
		},
		"spacing": {
			"margin": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "16px",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		},
		{
			"area": "uncategorized",
			"name": "main"
		},
		{
			"area": "hero",
			"name": "hero"
		}
	]
}