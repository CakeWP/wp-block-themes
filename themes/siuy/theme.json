{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [ "page", "post" ]
		},
		{
			"name": "without-title",
			"title": "Fullwidth without title",
			"postTypes": [ "page", "post" ]
		},
		{
			"name": "with-sidebar",
			"title": "Page with Sidebar",
			"postTypes": [ "page" ]
		}
	],
	"settings": {
		"border": {
			"style": true,
			"width": true,
			"color": true,
			"radius": true
		},
		"color": {
			"link": true,
			"defaultPalette": false,
			"defaultGradients": false,
			"duotone": [
				{
					"colors": [ "#edeae7", "#ff5252" ],
					"name": "Gray and red",
					"slug": "gray-red"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, rgba(0, 0, 0, 0.0001) 0%, #000000 100%)",
					"name": "Transparent to black",
					"slug": "transparent-black"
				}
			],
			"palette": [
				{
					"color": "#595959",
					"name": "Text",
					"slug": "text"
				},
				{
					"color": "#000",
					"name": "Text Dark",
					"slug": "text-dark"
				},
				{
					"color": "#878787",
					"name": "Text Light",
					"slug": "text-light"
				},
				{
					"color": "#d4d4d4",
					"name": "Text Lighter",
					"slug": "text-lighter"
				},
				{
					"color": "#ff5252",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#edeae7",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#a89b8e",
					"name": "Secondary Alt",
					"slug": "secondary-alt"
				},
				{
					"color": "#f9f9f9",
					"name": "Tile",
					"slug": "tile"
				},
				{
					"color": "#ebebeb",
					"name": "Separator",
					"slug": "separator"
				},
				{
					"color": "#979797",
					"name": "Border",
					"slug": "border"
				},
				{
					"color": "#fff",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#1d1f24",
					"name": "Dark",
					"slug": "dark"
				}
			]
		},
		"custom": {
			"spacing": {
				"side": "24px"
			}
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Cardo\", ui-sans",
					"name": "Cardo",
					"slug": "primary",
					"fontFace": [
						{
							"fontFamily": "Cardo",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Cardo-Regular.woff2" ]
						},
						{
							"fontFamily": "Cardo",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Cardo-Italic.woff2" ]
						},
						{
							"fontFamily": "Cardo",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Cardo-Bold.woff2" ]
						}
					]
				},
				{
					"fontFamily": "\"Lato\", ui-sans-serif",
					"name": "Lato",
					"slug": "secondary",
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-Light.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-Regular.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-Italic.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-Bold.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-BoldItalic.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-Black.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Lato-BlackItalic.woff2" ]
						}
					]
				},
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
					"slug": "system-sans-serif",
					"name": "System Sans Serif"
				},
				{
					"fontFamily": "ui-monospace, Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
					"slug": "system-monospace",
					"name": "System Monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Large",
					"size": "clamp(40px, 3.5vw, 64px)",
					"slug": "large"
				},
				{
					"name": "Medium",
					"size": "clamp(15px, 3.5vw, 20px)",
					"slug": "medium"
				},
				{
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"name": "Smaller",
					"size": "12px",
					"slug": "smaller"
				},
				{
					"name": "Smallest",
					"size": "10px",
					"slug": "smallest"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"elements": {
					"link": {
						"color": {
							"text": "inherit"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"spacing": {
					"padding": {
						"top": "0",
						"left": "0",
						"right": "0",
						"bottom": "0"
					},
					"margin": {
						"top": "0",
						"left": "0",
						"right": "0",
						"bottom": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"width": "0",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--light)"
				},
				"spacing": {
					"padding": {
						"top": "12px",
						"bottom": "12px",
						"left": "30px",
						"right": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--smaller)",
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontWeight": "700"
				}
			},
			"core/calendar": {
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--text)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--text)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/categories": {
				"elements": {
					"link": {
						"color": {
							"text": "inherit"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"spacing": {
					"padding": {
						"top": "0",
						"left": "0",
						"right": "0",
						"bottom": "0"
					},
					"margin": {
						"top": "0",
						"left": "0",
						"right": "0",
						"bottom": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "30px",
					"fontWeight": "700"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--smaller)",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"color": {
					"text": "var(--wp--preset--color--text-light)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "14px",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				}
			},
			"core/navigation-submenu": {
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--text)"
				}
			},
			"core/quote": {
				"border": {
					"style": "solid",
					"width": "1px 0",
					"color": "var(--wp--preset--color--separator)"
				},
				"spacing": {
					"padding": {
						"top": "30px",
						"bottom": "30px",
						"left": "0",
						"right": "0"
					}
				},
				"typography": {
					"fontSize": "clamp(29px, 3.5vw, 36px)"
				},
				"color": {
					"text": "var(--wp--preset--color--dark)"
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px 0",
					"color": "var(--wp--preset--color--separator)"
				},
				"spacing": {
					"padding": {
						"top": "60px",
						"bottom": "60px",
						"left": "0",
						"right": "0"
					}
				},
				"typography": {
					"fontSize": "clamp(29px, 3.5vw, 36px)"
				},
				"color": {
					"text": "var(--wp--preset--color--dark)"
				}
			},
			"core/code": {
				"border": {
					"width": "0",
					"radius": "0"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--text-light)"
				}
			},
			"core/table": {
				"border": {
					"style": "solid",
					"width": "2px",
					"color": "var(--wp--preset--color--separator)"
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--separator)"
				},
				"border": {
					"width": "1px"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"left": "0",
						"right": "0",
						"bottom": "0"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--smallest)",
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				},
				"color": {
					"text": "var(--wp--preset--color--text-lighter)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--smaller)",
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				},
				"color": {
					"text": "var(--wp--preset--color--text-light)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"elements": {
					"link": {
						"typography": {
							"fontSize": "80%",
							"textTransform": "uppercase"
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--style--block-gap)",
						"left": "0",
						"right": "0"
					}
				}
			},
			"core/post-comments-form": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"elements": {
					"h3": {
						"typography": {
							"fontSize": "16px",
							"fontFamily": "var(--wp--preset--font-family--secondary)",
							"textTransform": "uppercase"
						}
					}
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontWeight": "700",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			},
			"core/comment-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"color": {
					"background": "var(--wp--preset--color--tile)"
				},
				"spacing": {
					"padding": {
						"top": "15px",
						"left": "30px",
						"right": "30px",
						"bottom": "15px"
					}
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "16px",
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontWeight": "700"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--text)"
						}
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--text-lighter)"
