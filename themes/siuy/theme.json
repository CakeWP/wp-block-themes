				"elements": {
					"link": {
						"spacing": {
							"padding": {
								"top": "2px",
								"bottom": "2px",
								"left": "10px",
								"right": "10px"
							}
						},
						"border": {
							"style": "solid",
							"width": "1px 1px 1px 2px"
