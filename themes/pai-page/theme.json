  "styles": {
    "blocks": {
      "core/site-title": {
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      }
    },
    "color": {
      "background": "var:preset|color|bg-1",
      "text": "var:preset|color|text-1"
    },
    "css": "html {\n  font-size: var(--wp--preset--font-size--medium);\n  scroll-behavior: smooth;\n  scroll-padding-top: 80px;\n}\nbody {\n  line-height: 1.5;\n  -webkit-font-smoothing: auto;\n  -moz-osx-font-smoothing: auto;\n  overflow-x: hidden;\n}\n.wp-site-blocks {\n  display: flex;\n  flex-direction: column;\n  min-height: 100vh;\n}\n.admin-bar .wp-site-blocks {\n  min-height: calc(100vh - var(--wp-admin--admin-bar--height));\n}\n.header-fixed {\n  position: sticky;\n  top: 0;\n  z-index: 1000;\n}\n.admin-bar .header-fixed {\n  top: var(--wp-admin--admin-bar--height);\n}\n.text-center {\n  text-align: center;\n}\n.site-footer {\n  margin-top: auto;\n}\n@media (max-width: 991px) {\n  ._desktop {\n    display: none !important;\n  }\n}\n@media (min-width: 992px) {\n  ._mobile {\n    display: none !important;\n  }\n}\n",
    "elements": {
      "button": {
        "color": {
          "background": "var:preset|color|primary",
          "text": "var:preset|color|background"
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|primary"
        },
        "typography": {
          "textDecoration": "underline"
        }
      }
    },
    "spacing": {
      "blockGap": "var:preset|spacing|50",
      "padding": {
        "bottom": "0px",
        "left": "0px",
        "right": "0px",
        "top": "0px"
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|dm-sans"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ],
  "version": 2,
  "$schema": "https://schemas.wp.org/wp/6.2/theme.json"
}
