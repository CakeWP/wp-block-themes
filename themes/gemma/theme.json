{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "720px",
			"wideSize": "1400px"
		},
		"color": {
			"custom": true,
			"gradients": [
				{
					"slug": "primary-to-secondary",
					"gradient": "linear-gradient(-135deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Primary to secondary"
				},
				{
					"slug": "primary-to-tertiary",
					"gradient": "linear-gradient(-135deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--tertiary) 100%)",
					"name": "Primary to tertiary"
				},
				{
					"slug": "secondary-to-tertiary",
					"gradient": "linear-gradient(-135deg, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--tertiary) 100%)",
					"name": "Secondary to Tertiary"
				}
			],
			"palette": [
				{
					"color": "#FFF",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#00833a",
					"name": "Primary",
					"slug": "primary"
					
				},
				{
					"color": "#f3f9e4",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FFE382",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#FFFCF3",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.5rem"
					},
					"size": "1.6rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3.25rem"
					},
					"size": "3.25rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "4rem"
					},
					"size": "4rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.65rem",
						"max": "8rem"
					},
					"size": "8rem",
					"slug": "xx-large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Uncut Sans\", sans-serif",
					"name": "Uncut Sans",
					"slug": "uncut-sans",
					"fontFace": [
						{
							"fontFamily": "Uncut Sans",
							"fontStretch": "normal",
							"fontStyle": "oblique 0 11deg",
							"fontVariationSettings": "'ital' 0",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/uncut-sans/UncutSans-Variable.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Cormorant\", TimesNewRoman, \"Times New Roman\", Times, serif",
					"name": "Cormorant",
					"slug": "cormorant",
					"fontFace": [
						{
							"fontFamily": "Cormorant",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/cormorant/Cormorant[wght].woff2"
							]
						},
						{
							"fontFamily": "Cormorant",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/cormorant/Cormorant-Italic[wght].woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Arial, FreeSans, sans-serif",
					"name": "Arial",
					"slug": "arial"
				},
				{
					"fontFamily": "TimesNewRoman, \"Times New Roman\", Times, FreeSerif, \"Nimbus Roman D\", Baskerville, Georgia, serif",
					"name": "Times New Roman",
					"slug": "times-new-roman"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--quaternary)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"spacing": {
			"blockGap": "2.5rem",
			"padding": {
				"top": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--40)",
				"bottom": "var(--wp--preset--spacing--30)",
				"left": "var(--wp--preset--spacing--40)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
			"fontWeight": "500",
			"lineHeight": "1.5",
			"fontSize": "var(--wp--preset--font-size--medium)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
					"fontWeight": "500",
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--times-new-roman)",
					"fontWeight": "500",
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": {"ref": "styles.elements.h2.typography.fontFamily"},
					"fontWeight": {"ref": "styles.elements.h2.typography.fontWeight"},
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
					"fontWeight": "700",
					"lineHeight": "1.5",
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
					"fontWeight": "800",
					"lineHeight": "1.2",
					"textTransform": "uppercase",
					"letterSpacing": "2px",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
					"fontWeight": "800",
					"lineHeight": "1.2",
					"textTransform": "uppercase",
					"letterSpacing": "2px",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"button": {
				"border": {
					"radius": "9999px"
				},
				"spacing": {
					"padding": {
						"top": "0.7rem !important",
						"right": "1.5rem !important",
						"bottom": "0.7rem !important",
						"left": "1.5rem !important"
					}
				},
				"typography": {
					"lineHeight": "1",
					"textDecoration": "none"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "underline 1px solid"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "underline 1px solid"
					}
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dotted"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
					"fontWeight": "500",
					"lineHeight": "1.2",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/navigation": {
				"css": "&-item.current-menu-item { text-decoration: underline }",
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dotted"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/navigation-submenu": {
				"css": "&:not(.has-background).wp-block-navigation__submenu-container { background: var(--wp--preset--color--secondary); border: none; }"
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "1.618rem"
				}
			},
			"core/button": {
				"css": "&:hover {opacity:0.8;}"
			},
			"core/details": {
				"border": {
					"width": "2px",
					"style": "solid",
					"radius": "2rem"
				},
				"spacing": {
					"padding": {
						"top": "0.7rem",
						"right": "1.5rem",
						"bottom": "0.7rem",
						"left": "1.5rem"
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--times-new-roman)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"border": {
					"width": "0 0 0 2px",
					"color": "var(--wp--preset--color--primary)",
					"style": "solid"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"textTransform": "uppercase",
							"letterSpacing": "0.07em"
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				},
				"variations": {
					"plain": {
						"border": {
							"width": "0"
						},
						"spacing": {
							"padding": {
								"left": "var(--wp--preset--spacing--30)",
								"right": "var(--wp--preset--spacing--30)"
							}
						}
					}
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--times-new-roman)"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"border": {
					"style": "solid",
					"width": "2px 0",
					"color": "var(--wp--preset--color--primary)"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--uncut-sans)",
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"textTransform": "uppercase",
							"letterSpacing": "0.07em"
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--30)"
					}					
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "1.618rem",
						"top": "1.618rem"
					}
				},
				"typography": {
					"fontWeight": "500"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dotted"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"letterSpacing": "0.07em"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"letterSpacing": "0.07em"
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)",
							"fontWeight": "500"
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/search": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--uncut-sans)"
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-no-title",
			"title": "Page without title"
		}
	]
}
