				{
					"fontFamily": "'Eb Garamond'",
					"name": "Eb Garamond",
					"slug": "primary"
				},
				{
					"fontFamily": "'Josefin Sans'",
					"name": "Josefin Sans",
					"slug": "secondary"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "16px",
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "18px",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "20px",
					"slug": "normal"
				},
				{
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"name": "Extra large",
					"size": "40px",
