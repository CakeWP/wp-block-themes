{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		},
		{
			"name": "archive-product",
			"postTypes": [
				"page"
			],
			"title": "Archive Producut"
		},
		{
			"name": "single-product",
			"postTypes": [
				"product"
			],
			"title": "Single Product"
		},
		{
			"name": "product-search-results",
			"postTypes": [
				"product"
			],
			"title": "Product Search Results"
		},
		{
			"name": "blog-posts",
			"postTypes": [
				"page"
			],
			"title": "Blog Posts"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#DA4F28",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#F5F6FC",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f6f6f6",
					"name": "Secondary Alt",
					"slug": "secondary-alt"
				},
				{
					"color": "#666666",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#333333",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#ebebeb",
					"name": "Border",
					"slug": "border"
				},
				{
					"color": "#999999",
					"name": "Meta",
					"slug": "meta"
				}
			]
		},
		"layout": {
			"contentSize": "760px",
			"wideSize": "1400px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.75rem, 3.5vw, 2.1875rem)",
					"slug": "30",
					"name": "1. (28px-35px)"
				},
				{
					"size": "clamp(2.1875rem, 3.5vw, 2.625rem)",
					"slug": "40",
					"name": "2.(35px-42px)"
				},
				{
					"size": "clamp(1.875rem, 3.5vw, 3.0625rem)",
					"slug": "50",
					"name": "3. (30px-49px)"
				},
				{
					"size": "clamp(2.1875rem, 3.5vw, 3.5rem)",
					"slug": "60",
					"name": "4. (35px-56px)"
				},
				{
					"size": "clamp(2.1875rem, 3.5vw, 3.9375rem)",
					"slug": "70",
					"name": "5. (35px-63px)"
				},
				{
					"size": "clamp(2.625rem, 4.5vw, 4.375rem)",
					"slug": "80",
					"name": "6. (42px-70px)"
				},
				{
					"size": "clamp(3.5rem, 5.5vw, 6.5625rem)",
					"slug": "90",
					"name": "7. (56px-105px)"
				},
				{
					"size": "clamp(3.5rem, 6.5vw, 7.5rem)",
					"slug": "100",
					"name": "8. (56px-120px)"
				},
				{
					"size": "clamp(3.5rem, 7.5vw, 11.25rem)",
					"slug": "110",
					"name": "9. (56px-180px)"
				},
				{
					"size": "clamp(7.5rem, 8.5vw, 16.25rem)",
					"slug": "120",
					"name": "10. (120px-260px)"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": false,
					"size": "0.75rem",
					"slug": "tiny",
					"name": "Tiny (12px)"
				},
				{
					"fluid": false,
					"size": "0.8125rem",
					"slug": "x-small",
					"name": "X-Small (13px)"
				},
				{
					"fluid": false,
					"size": "0.875rem",
					"slug": "small",
					"name": "Small (14px)"
				},
				{
					"fluid": false,
					"size": "1rem",
					"slug": "normal",
					"name": "Normal (16px)"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium",
					"name": "Medium (18px)"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "large",
					"name": "Large (20px)"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "x-large",
					"name": "X-Large (24px)"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.75rem"
					},
					"size": "1.75rem",
					"slug": "xx-large",
					"name": "XX-Large (28px)"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.25rem"
					},
					"size": "2.25rem",
					"slug": "xxx-large",
					"name": "XXX-Large (36px)"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3.0625rem"
					},
					"size": "3.0625rem",
					"slug": "huge",
					"name": "Huge (49px)"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "3.625rem"
					},
					"size": "3.625rem",
					"slug": "colossal",
					"name": "Colossal (58px)"
				},
				{
					"fluid": {
						"min": "2.625rem",
						"max": "4.375rem"
					},
					"size": "4.375rem",
					"slug": "gigantic",
					"name": "Gigantic (70px)"
				},
				{
					"fluid": {
						"min": "4.375rem",
						"max": "7.5rem"
					},
					"size": "7.5rem",
					"slug": "enormous",
					"name": "Enormous (120px)"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"spacing": {
					"blockGap": "0"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)"
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							},
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						},
						"spacing": {
							"padding": {
								"top": "14px",
								"right": "14px",
								"bottom": "14px",
								"left": "14px"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny)"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "1.3125rem",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comments-title":{
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px 0"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"typography": {
					"lineHeight": "1.3"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40) !important",
						"top": "var(--wp--preset--spacing--40) !important"
					}
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						"color": {
							"background": "var(--wp--preset--color--primary)",
							"text": "var(--wp--preset--color--base)"
						},
						"border": {
							"radius": "6px"
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							},
							"color": {
								"background": "var(--wp--preset--color--contrast)",
								"text": "var(--wp--preset--color--base)"
							}
						},
						"spacing": {
							"padding": {
								"bottom": "clamp(12px, 3.5vw, 14px)",
								"left": "clamp(12px, 3.5vw, 28px)",
								"right": "clamp(12px, 3.5vw, 28px)",
								"top": "clamp(12px, 3.5vw, 14px)"
							}
						}
					}

				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "700",
					"lineHeight": "1.35"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"typography": {
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"border": {
					"radius": "6px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				"spacing": {
					"padding": {
						"bottom": "clamp(12px, 3.5vw, 14px)",
						"left": "clamp(21px, 3.5vw, 28px)",
						"right": "clamp(21px, 3.5vw, 28px)",
						"top": "clamp(12px, 3.5vw, 14px)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--colossal)",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"lineHeight": "1.35"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.45"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.56"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					},
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "footer",
			"name": "footer-has-widgets",
			"title": "Footer Has Widgets (Default)"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		}
	]
}
