{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"title": "Single + Sidebar",
			"name": "single-sidebar",
			"postTypes": [
				"post"
			]
		},
		{
			"title": "Page (without title)",
			"name": "page-without-title",
			"postTypes": [
				"page"
			]
		}
	],
    "settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#1AAD64",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#2750E1",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FFE156", 
					"name": "Lemmonary",
					"slug": "lemmonary"
				},
				{
					"color": "#2C2C2C",
					"name": "Body",
					"slug": "body"
				},
				{
					"color": "#FCFCFC",
					"name": "Body Alt",
					"slug": "body-alt"
				},
				{
					"color": "#1C1C1C",
					"name": "Paragraph",
					"slug": "paragraph"
				},
				{
					"color": "#757575",
					"name": "Paragraph Alt",
					"slug": "paragraph-alt"
				},
				{
					"color": "#5C5C5C",
					"name": "Meta",
					"slug": "meta"
				},
				{
					"color": "#7C7C7C",
					"name": "Border",
					"slug": "border"
				},
				{
					"color": "#E0E0E0",
					"name": "Border Alt",
					"slug": "border-alt"
				},
				{
					"color": "#F7F7F7",
					"name": "Midground",
					"slug": "midground"
				},
				{
					"color": "#ececec",
					"name": "Midground Alt",
					"slug": "midground-alt"
				},
				{
					"color": "#FFFFFF",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#111111",
					"name": "Background Alt",
					"slug": "background-alt"
				},
				{
					"color": "#ffffff00",
					"name": "Transparent",
					"slug": "transparent"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
        "layout": {
            "contentSize": "774px",
            "wideSize": "1600px"
        },
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "0.5rem",
					"slug": "10",
					"name": "8px"
				},
				{
					"size": "clamp(0.875rem, 0.875rem + ((1vw - 0.48rem) * 0.24), 1rem)",
					"slug": "20",
					"name": "16px"
				},
				{
					"size": "clamp(1.125rem, 1.125rem + ((1vw - 0.48rem) * 0.721), 1.5rem)",
					"slug": "30",
					"name": "24px"
				},
				{
					"size": "clamp(1.5rem, 1.5rem + ((1vw - 0.48rem) * 0.962), 2rem)",
					"slug": "40",
					"name": "32px"
				},
				{
					"size": "clamp(2.25rem, 2.25rem + ((1vw - 0.48rem) * 1.442), 3rem)",
					"slug": "50",
					"name": "48px"
				},
				{
					"size": "clamp(3rem, 3rem + ((1vw - 0.48rem) * 1.923), 4rem)",
					"slug": "60",
					"name": "64px"
				},
				{
					"size": "clamp(3.75rem, 3.75rem + ((1vw - 0.48rem) * 2.404), 5rem)",
					"slug": "70",
					"name": "80px"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontWeight": true,
			"fontStyle": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Plus Jakarta Sans\", sans-serif",
					"name": "Plus Jakarta Sans",
					"slug": "plus-jakarta",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Plus Jakarta Sans",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/jakarta/PlusJakartaSans-Regular.woff2" ]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Plus Jakarta Sans",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/jakarta/PlusJakartaSans-Medium.woff2" ]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Plus Jakarta Sans",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/jakarta/PlusJakartaSans-SemiBold.woff2" ]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Plus Jakarta Sans",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/jakarta/PlusJakartaSans-Bold.woff2" ]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Plus Jakarta Sans",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/jakarta/PlusJakartaSans-ExtraBold.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Tiny, 16px",
					"slug": "tiny",
					"size": "1rem",
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					}
				},
				{
					"name": "Tiny Plus, 18px",
					"slug": "tiny-plus",
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					}
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "small",
					"name": "Small, 20px"
				},
				{
					"fluid": {
						"min": "1.35rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "small-plus",
					"name": "Small Plus, 24px"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.875rem"
					},
					"size": "1.875rem",
					"slug": "medium",
					"name": "Medium, 30px"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.25rem"
					},
					"size": "2.25rem",
					"slug": "medium-plus",
					"name": "Medium Plus, 36px"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "3rem"
					},
					"size": "3rem",
					"slug": "large",
					"name": "Large, 48px"
				},
				{
					"fluid": {
						"min": "2.75rem",
						"max": "3.5rem"
					},
					"size": "3.5rem",
					"slug": "large-plus",
					"name": "Large Plus, 56px"
				},
				{
					"fluid": {
						"min": "2.85rem",
						"max": "4.5rem"
					},
					"size": "4.5rem",
					"slug": "huge",
					"name": "Huge, 72px"
				},
				{
					"fluid": {
						"min": "3.15rem",
						"max": "5rem"
					},
					"size": "5rem",
					"slug": "huge-plus",
					"name": "Huge Plus (80px)"
				},
				{
					"fluid": {
						"min": "3.5rem",
						"max": "6rem"
					},
					"size": "6rem",
					"slug": "gigantic",
					"name": "Gigantic"
				}
			]
		}
    },
    "styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--body)"
		},
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny-plus)"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--border)"
				},
				"border": {
					"width": "0 0 1px 0"
				}
			},
			"core/post-title": {
				"typography": {
					"lineHeight": "1.3",
					"fontSize": "var(--wp--preset--font-size--large-plus)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny)"
				}
			},
            "core/columns": {
                "spacing": {
                    "blockGap": "50px"
                }
            },
			"core/paragraph": {
				"color": {
					"text": "var(--wp--preset--color--paragraph)"
				}
			}
		},
		"elements": {
			"button": {
				"border": {
					"radius": "9999px",
					"width": "0px"
				},
				"spacing": {
					"padding": {
						"top": "1.334em",
						"right": "2.667em",
						"bottom": "1.334em",
						"left": "2.667em"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--background-alt)",
					"text": "var(--wp--preset--color--body-alt)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--body-alt)"
					}
				},
				"typography": {
					"lineHeight": "1",
					"fontSize": "var(--wp--preset--font-size--tiny-plus)",
					"fontWeight": "700"
				}
			},
			"link": { 
				"color": {
					"text": "inherit"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary?)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "1.3"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large-plus)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small-plus)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny-plus)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.3"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
            "padding": {
                "top": "0px",
                "right": "7vw",
                "bottom": "0px",
                "left": "7vw"
            }
		},
        "typography": {
			"fontFamily": "var(--wp--preset--font-family--plus-jakarta)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.6"
		}
    },
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer", 
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	]
}